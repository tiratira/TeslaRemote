<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".NavShareActivity"
        android:layout_marginBottom="18dp"
        android:background="@drawable/card_background"
        android:orientation="vertical"
        android:clipChildren="true"
        android:id="@+id/mapShareLayout"
        android:layout_marginTop="18dp" android:layout_marginLeft="18dp" android:layout_marginRight="18dp">

    <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:layout_marginTop="56dp"
            android:id="@+id/scene_root"/>
    <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/holo_red_dark"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/mapsearchbar"
            app:navigationIcon="@drawable/ic_back"
            android:elevation="4dp"
            app:title="发送地址" app:titleTextAppearance="@style/Toolbar.TitleText"/>

</androidx.coordinatorlayout.widget.CoordinatorLayout>